{
  "name": "-flatism",
  "version": "0.0.0",
  "description": "non-flat",
  "main": "webpack.config.js",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint .",
    "format": "prettier-eslint --write '**/*.js'",
    "minify:css": "uglifycss public/stylesheets/style.css --output public/stylesheets/style.css",
    "build": "npm-run-all -s build:prod minify:css",
    "build:prod": "cross-env NODE_MODE=production webpack",
    "build:dev": "cross-env NODE_MODE=development webpack",
    "watch": "cross-env NODE_MODE=development webpack --watch",
    "serve": "webpack-dev-server"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint"
    ]
  },
  "keywords": [
    "null223",
    "css animation"
  ],
  "repository": "git@github.com:null223/-flatism.git",
  "author": "null__me <sumida0223@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "husky": "^7.0.1",
    "import-glob-loader": "^1.1.0",
    "lint-staged": "^11.1.2",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "sass-loader": "^12.1.0",
    "uglifycss": "^0.0.29",
    "url-loader": "^4.1.1",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
